{
    "name": "boenrobot/composer-install-library-of-functions",
    "description": "A Composer plugin to handle the \"functionmap\" autoload section.",
    "type": "composer-plugin",
    "license": "LGPL-2.1-only",
    "authors": [
        {
            "name": "Vasil Rangelov",
            "email": "boen.robot@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "boenrobot\\": "src/boenrobot"
        }
    },
    "extra": {
        "class": "boenrobot\\Composer\\FunctionHandler\\Plugin"
    },
    "require": {
        "composer-plugin-api": "^1.0"
    },
    "require-dev": {
        "composer/composer": "^1.3"
    }
}
